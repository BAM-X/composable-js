{
  "env": {
    "browser": true
  },
  "globals": {
    "describe": true,
    "beforeAll": true,
    "afterAll": true,
    "it": true,
    "expect": true,
    "test": false,
    "ok": false,
    "is": false,
    "equal": false,
    "deepEqual": false,
    "strictEqual": false,
    "notStrictEqual": false,
    "notEqual": false,
    "throws": false,
    "asyncTest": false,
    "start": false,
    "QUnit": false
  },
  "rules": {
    "brace-style": 0,
    "dot-notation": 1,
    "no-new-wrappers": 0,
    "no-sparse-arrays": 0,
    "no-extend-native": 0,
    "new-cap": 1
  }
}
